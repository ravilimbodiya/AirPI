        <!--signup modal window START-->
        <div id="signupModal" class="modal fade" role="dialog" >
              <div class="modal-dialog  modal-md">

                <!-- Modal content-->
                <div class="modal-content">
                  <div class="modal-header" style="background-color: #43bfc7; border-radius: 5px;">
                    <button type="button" class="close" data-dismiss="modal" style="margin-top: -5px;">&times;</button>
                    <h3 class="modal-title">Register to AirPI</h3>
                  </div>
                  <div class="modal-body" style="padding: 30px 150px 0;">
                     
                        <div class="" style="text-align: center;  padding: 0px 0 10px 0;">
                            <a href='/'></a>
                        </div>
                            
                        <div style="text-align: center;">
                            <input type="text" placeholder="First Name"  ng-model="firstName" autocomplete="firstName" style="width: 95%;" class="form-control input-lg" required><br>
                            <input type="text" placeholder="Last Name" ng-model="lastName" autocomplete="lastName" style="width: 95%;" class="form-control input-lg" required><br>

                            <input type="email" placeholder="Email" ng-model="email" autocomplete="email" style="width: 95%;" class="form-control input-lg" required><br>
                            <input type="password" placeholder="Password" ng-model="pass" autocomplete="password" style="width: 95%;" class="form-control  input-lg" required>
                            <input type="password" placeholder="Confirm Password" autocomplete="password" style="width: 95%;" class="form-control input-lg" required><br>
                            <input type="text" placeholder="City" ng-model="city" autocomplete="city" style="width: 95%;" class="form-control input-lg" required><br>
                            <input type="submit" class="btn btn-info btn-md" value="Register" ng-click="submitRegister();" style=""><br><br>
                        </div>
                        
                        
                      </div>
                      
                      
                    </div>

              </div>
            </div>
        <!--login modal window START-->
        <div id="loginModal" class="modal fade" role="dialog" >
              <div class="modal-dialog  modal-md">

                <!-- Modal content-->
                <div class="modal-content">
                  <div class="modal-header" style="background-color: #43bfc7; border-radius: 5px;">
                    <button type="button" class="close" data-dismiss="modal" style="margin-top: -5px;">&times;</button>
                    <h3 class="modal-title">Log in to AirPI</h3>
                  </div>
                  <div class="modal-body" style="padding: 30px 150px 0;">
                     
                        <div class="" style="text-align: center;  padding: 0px 0 10px 0;">
                            <a href='/'></a>
                        </div>
                            
                        <div>
                            <input type="email" placeholder="Email" ng-model="emailLogin" autocomplete="email" style="width: 95%;" class="form-control input-lg" required><br>
                            <input type="password" placeholder="Password" ng-model="password" autocomplete="password" style="width: 95%;" class="form-control  input-lg" required>
                            <input type="submit" class="btn btn-info btn-md" value="Log in" ng-click="submitLogin();" style="float:right;"><br><br>
                        </div>
                        
                        
                      </div>
                      
                      
                    </div>

              </div>
